#PostgreSQL
DB_TYPE=
DB_HOST=
DB_PORT=
DB_BANCO=
DB_USERNAME=
DB_PASSWORD=
DB_DRIVER=org.postgresql.Driver
DB_URL=jdbc:postgresql://localhost:5432/desafio
DB_DIALECT=org.hibernate.dialect.PostgreSQLDialect

spring.datasource.url=${DB_URL}
spring.datasource.username=${DB_USERNAME}
spring.datasource.password=${DB_PASSWORD}
spring.datasource.driver-class-name=${DB_DRIVER}
spring.jpa.properties.hibernate.dialect=${DB_DIALECT}

spring.flyway.sql-server.application-lock=false

# Gmail para recebimento dos resultados qd terminar a votacao
GMAIL_PORT=587
GMAIL_EMAIL=seu.email@gmail.com
GMAIL_PASSWORD=senha gerada pelo gmail para dispositivos (gerar no link https://myaccount.google.com/security)

spring.mail.host=smtp.gmail.com
spring.mail.port=${GMAIL_PORT}
spring.mail.username=${GMAIL_EMAIL}
spring.mail.password=${GMAIL_PASSWORD}
spring.mail.properties.mail.smtp.auth=true
spring.mail.properties.mail.smtp.starttls.enable=true
spring.mail.properties.mail.smtp.starttls.required=true
